    "recipeYield": "1 serving",
    "recipeIngredient": [
        "\u00bd cup Egg Beaters",
        "2 tablespoons skim milk",
        "1 teaspoon cinnamon",
        "\u00bd teaspoon vanilla extract",
        "1  serving splash vegetable spray",
        "3 slices whole-wheat bread"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine the Egg Beaters, milk, cinnamon and vanilla in a bowl.  Beat with a whisk.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Heat nonstick pan (medium heat should do) and coat it with vegetable spray.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Coat bread with egg mixture.  Add to pan and lightly brown on both sides.\n"
        }
    ],
