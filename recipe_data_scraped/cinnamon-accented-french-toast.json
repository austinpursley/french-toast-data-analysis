    "recipeIngredient": [
        "3 eggs, beaten",
        "2 teaspoons white sugar",
        "1 teaspoon milk",
        "1 teaspoon ground cinnamon",
        "\u00bc teaspoon salt",
        "6 slices bread"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Mix beaten eggs, sugar, milk, cinnamon, and salt in a large, shallow bowl. Dip bread slices into egg mixture to coat.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Heat a skillet over medium heat. Cook bread in hot skillet until browned completely, 2 to 3 minutes per side.\n"
        }
    ],
