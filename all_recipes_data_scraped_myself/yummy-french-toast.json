    "recipeYield": "8 servings",
    "recipeIngredient": [
        "3  large eggs large eggs",
        "\u2154 cup milk",
        "2 tablespoons granulated sugar",
        "1 teaspoon cinnamon",
        "\u00bd teaspoon nutmeg",
        "1 teaspoon vanilla",
        "8 slices bread",
        "2 tablespoons canola oil"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat electric griddle to 325 F (or medium heat).\n"
        },
        {
            "@type": "HowToStep",
            "text": "Whisk together eggs, milk, sugar, cinnamon, nutmeg, and vanilla in a medium bowl. Set aside.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Place bread in a shallow baking dish and pour egg mixture over bread. Let soak for about 10-15 seconds.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Heat 1 tablespoon of canola oil in the griddle. Cook the first four slices for about 6 minutes or until golden brown, turning once. Repeat.\n"
        }
    ],
