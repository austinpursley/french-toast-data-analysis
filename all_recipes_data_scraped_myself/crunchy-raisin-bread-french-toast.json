    "recipeYield": "4 servings",
    "recipeIngredient": [
        "\u00be cup egg substitute",
        "\u2153 cup skim milk",
        "1 teaspoon vanilla",
        "2 tablespoons powdered sugar",
        "\u215b teaspoon ground nutmeg",
        "2 cups whole grain wheat and barley cereal or coarsely crushed bran flakes ",
        "8 slices Sun-Maid\u00ae Raisin Bread",
        "1 tablespoon Powdered sugar for topping"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Mix egg substitute, milk, vanilla, powdered sugar and nutmeg in pie plate.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Place cereal in another pie plate.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Spray 12-inch nonstick skillet generously with cooking spray. Heat over medium heat. Dip both sides of 4 slices of the bread in egg mixture, then in cereal.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Cook 2 to 3 minutes or until golden brown, turning once. Repeat with remaining bread, egg mixture and cereal, spraying skillet each time.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Serve topped with powdered sugar, if desired.\n"
        }
    ],
