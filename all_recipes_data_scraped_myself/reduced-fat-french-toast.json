    "recipeIngredient": [
        "\u00bd cup egg substitute",
        "\u2154 cup skim milk",
        "1 teaspoon vanilla extract",
        "\u00bd teaspoon ground cinnamon",
        "6 slices reduced calorie white bread"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Beat together egg substitute, milk, vanilla and cinnamon. Dip bread slices in egg mixture until both sides are soaked.\n"
        },
        {
            "@type": "HowToStep",
            "text": "Spray a skillet or frying pan with cooking spray and heat over medium high heat. Place bread slices into pan and cook until golden brown on both sides\n"
        }
    ],
